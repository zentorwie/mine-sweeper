{
  "presets": [
    "@babel/env",
    "@babel/typescript",
    "@babel/react",
    [ "@babel/es2015", { "modules": false } ]
  ],
  "plugins": [
    "react-hot-loader/babel",
    "@babel/proposal-object-rest-spread",
    "transform-decorators-legacy",
    [
      "transform-class-properties", { "loose": true }
    ],
    ["babel-plugin-styled-components", {
      "minify": false
    }]
  ]
}